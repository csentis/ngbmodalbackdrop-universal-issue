{
    "name": "pwa",
    "description": "Modular progressive web app based on Angular 5.",
    "version": "0.0.0",
    "keywords": [
        "angular",
        "frontend",
        "event-based",
        "progressive web app",
        "service worker"
    ],
    "homepage": "https://www.test.abc",
    "author": {
        "name": "Christian Sentis",
        "email": "christian.sentis@outlook.com"
    },
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": "^8.9.0",
        "npm": "^5.5.0"
    },
    "scripts": {
        "ng": "ng",
        "build": "ng build --prod",
        "ngsw": "run-s ngsw:*",
        "ngsw:serve": "http-server dist/browser -p 8080",
        "rimraf:dist": "rimraf dist",
        "start": "npm run start:aot",
        "start:aot": "ng serve --aot",
        "start:dev": "ng serve --dev",
        "universal": "run-s universal:clean universal:build:* universal:bundle:* universal:serve",
        "universal:ci": "run-s universal:clean universal:build:* universal:bundle:*",
        "universal:clean": "npm run rimraf:dist",
        "universal:build:client": "ng build --prod ",
        "universal:build:server": "ng build --prod --app 1 --output-hashing=false",
        "universal:bundle:server": "webpack --config webpack.server.config.js --progress --colors",
        "universal:serve": "node dist/server.js"
    },
    "dependencies": {
        "@angular/animations": "^5.1.0",
        "@angular/common": "^5.1.0",
        "@angular/compiler": "^5.1.0",
        "@angular/core": "^5.1.0",
        "@angular/forms": "^5.1.0",
        "@angular/http": "^5.1.0",
        "@angular/platform-browser": "^5.1.0",
        "@angular/platform-browser-dynamic": "^5.1.0",
        "@angular/platform-server": "^5.1.0",
        "@angular/router": "^5.1.0",
        "@angular/service-worker": "^5.1.0",
        "@ng-bootstrap/ng-bootstrap": "~1.0.0-beta.7",
        "@nguniversal/express-engine": "^5.0.0-beta.5",
        "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
        "bootstrap": "~4.0.0-beta.2",
        "core-js": "^2.5.1",
        "reflect-metadata": "^0.1.10",
        "rxjs": "~5.5.5",
        "ts-loader": "^3.2.0",
        "xhr2": "^0.1.4",
        "zone.js": "^0.8.18"
    },
    "devDependencies": {
        "@angular/cli": "^1.6.0",
        "@angular/compiler-cli": "^5.1.0",
        "@angular/language-service": "^5.1.0",
        "@types/core-js": "^0.9.43",
        "@types/express": "^4.0.39",
        "@types/node": "^8.0.57",
        "@types/reflect-metadata": "^0.0.5",
        "@types/requirejs": "^2.1.31",
        "@types/tether": "^1.4.3",
        "@types/webpack": "^3.8.0",
        "chalk": "^2.3.0",
        "codelyzer": "^4.0.1",
        "debug": "^3.1.0",
        "http-server": "^0.10.0",
        "node-sass": "^4.7.2",
        "npm-run-all": "^4.1.2",
        "rimraf": "^2.6.2",
        "rxjs-tslint-rules": "^3.3.0",
        "sass-lint": "^1.12.1",
        "ts-node": "^3.3.0",
        "tslint": "^5.8.0",
        "tslint-loader": "^3.5.3",
        "typescript": "~2.5.3"
    }
}
